<?xml version="1.0"?>
<suppressions>

    <!-- Suppress missing includes -->
    <suppress>
        <id>missingInclude</id>
    </suppress>
    <suppress>
        <id>missingIncludeSystem</id>
    </suppress>

    <!-- Suppress unused functions -->
    <suppress>
        <id>unusedFunction</id>
    </suppress>

    <!-- Suppress static linkage for functions to be used outside of translation unit -->
    <suppress>
        <id>staticFunction</id>
        <symbolName>BspMotor_SetDirection</symbolName>
    </suppress>
    <suppress>
        <id>staticFunction</id>
        <symbolName>BspMotor_SetDutyCycle</symbolName>
    </suppress>
    <suppress>
        <id>staticFunction</id>
        <symbolName>Rover4ws_SampleEncoders</symbolName>
    </suppress>
    <suppress>
        <id>staticFunction</id>
        <symbolName>BspAdc_ReadRaw</symbolName>
    </suppress>

    <!-- Suppress double sign comparison portability error -->
    <suppress>
        <id>invalidPointerCast</id>
        <fileName>*/bots/src/rover_4ws.c</fileName>
    </suppress>

</suppressions>